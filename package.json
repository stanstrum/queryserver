{
  "name": "queryserver",
  "private": false,
  "version": "4.0.1",
  "description": "The Typescript rewrite of QueryServer3 :)",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build:js",
    "build:types": "tsc -p tsconfig-build.json --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --plugins @babel/plugin-proposal-numeric-separator --extensions \".ts,.tsx\" --source-maps inline",
    "test": "jest --runInBand --expand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stanstrum/queryserver.git"
  },
  "author": "stanstrum",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stanstrum/queryserver/issues"
  },
  "homepage": "https://github.com/stanstrum/queryserver#readme",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-numeric-separator": "^7.18.6",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.16.0",
    "@types/varint": "^6.0.1",
    "babel-plugin-root-import": "^6.6.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.3",
    "zod-validation-error": "^1.3.0"
  },
  "dependencies": {
    "hexy": "^0.3.4",
    "tslib": "^2.5.0",
    "varint": "^6.0.0",
    "zod": "^3.21.4"
  }
}
